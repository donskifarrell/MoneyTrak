template(name='main_content')
  {{#if currentUser}}
    {{#if isPageSelected 'import-data'}}
        {{> import_data}}
    {{/if}}
    {{#if isPageSelected 'transactions-view'}}
        {{> transactions_view}}
    {{/if}}
    {{#if isPageSelected 'graph-view'}}
        {{> graph_view}}
    {{/if}}
    {{#if isPageSelected 'location_view'}}
        {{> location_view}}
    {{/if}}
  {{else}}
    {{> landing_page}}
  {{/if}}

template(name='import_data')
    h2 Import Data
    hr

template(name='location_view')
    h2 Location Overview
    hr
